<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lingzhen.myproject.lifefolder.mapper.UserMapper">

    <insert id="save" parameterType="map" useGeneratedKeys="true" keyProperty="userId" >
        INSERT INTO sys_user(userName,`password`,nickname,phoneNumber,createTime,createDate)
        VALUE (#{userName},#{password},#{nickname},#{phoneNumber},#{createTime},#{createDate})
    </insert>


</mapper>
